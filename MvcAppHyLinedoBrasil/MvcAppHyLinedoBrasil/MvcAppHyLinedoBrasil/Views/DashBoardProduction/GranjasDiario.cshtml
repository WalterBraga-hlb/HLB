@model MvcAppHyLinedoBrasil.Models.DropDownListDashBoard

@{
    ViewBag.Title = "Dashboard - Granjas - Diário";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("//code.jquery.com/ui/1.11.0/themes/smoothness/jquery-ui.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("//code.jquery.com/jquery-1.10.2.js")" type="text/javascript"></script>
<script src="@Url.Content("//code.jquery.com/ui/1.11.0/jquery-ui.js")" type="text/javascript"></script>

<h3>Dashboard - Granjas - Diário</h3>
    @Html.Partial("Abas")
<table>
    <tr>
        <td>
            <div>
            @using (Html.BeginForm("AtualizaGranjasDiario", "DashBoardProduction", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                @Html.Partial("ParametrosGranjas")
            }
            @Html.Partial("AbasPeriodoGranjas")
            </div>
        </td>
    </tr>
    <tr>
        <td>
            @Html.Partial("OvosIncubaveisDiario")
        </td>
    </tr>
    <tr>
        <td>
            @Html.Partial("OvosSujosDiario")
        </td>
    </tr>
    <tr>
        <td>
            @Html.Partial("OvosTrincadosDiario")
        </td>
    </tr>
    <tr>
        <td>
            @Html.Partial("MediaIdadeLotesDiario")
        </td>
    </tr>
</table>